   "source": [
    "import numpy as np\n",
    "\n",
    "A = np.array([0, 5, 10, 15, 20],dtype=int)\n",
    "\n",
    "def search_sort(A, x):\n",
    "    n = len(A)\n",
    "    for j in range(n):\n",
    "        if int(x) == int(A[j]):\n",
    "           return j\n",
    "\n",
    "x = input(\"Enter a number to search: \")\n",
    "solution = search_sort(A, x)\n",
    "print(solution)"
   ]
